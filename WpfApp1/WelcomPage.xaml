<Window x:Class="WpfApp1.WelcomPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
        Title="WelcomPage" Height="450" Width="800">
    <Grid x:Name="Gavno">
        <DataGrid x:Name="Piska">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Id" Binding="{Binding id}"/>
                <DataGridTextColumn Header="name" Binding="{Binding name}"/>
                <DataGridTextColumn Header="login" Binding="{Binding login}"/>
                <DataGridTextColumn Header="password" Binding="{Binding password}"/>
                <DataGridTextColumn Header="ip" Binding="{Binding ip}"/>
                <DataGridTextColumn Header="last" Binding="{Binding lastenter}"/>
                <DataGridTextColumn Header="serv" Binding="{Binding services}"/>
                <DataGridTextColumn Header="tyyyp" Binding="{Binding type}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
